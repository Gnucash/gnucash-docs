<t:templates xmlns:t="http://nwalsh.com/docbook/xsl/template/1.0"
             xmlns:param="http://nwalsh.com/docbook/xsl/template/1.0/param"
             xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

<t:titlepage element="book" wrapper="div" class="titlepage">
  <t:titlepage-content side="recto">
    <title/>
    <subtitle/>
    <authorgroup/>
    <author/>
    <publisher/>
	<copyright/>
    <legalnotice/>
    <revhistory/>
    <revision/>
    <releaseinfo/>
  </t:titlepage-content>

  <t:titlepage-content side="verso">
  </t:titlepage-content>

  <t:titlepage-separator>
  </t:titlepage-separator>

  <t:titlepage-before side="recto">
  </t:titlepage-before>

  <t:titlepage-before side="verso">
    <img align ="left" src="stylesheet/gnucash-icon.png" alt="GnuCash Logo"/>
  </t:titlepage-before>
</t:titlepage>

<t:titlepage element="article" wrapper="div" class="titlepage">
  <t:titlepage-content side="recto">
    <title/>
    <subtitle/>
    <authorgroup/>
    <author/>
    <publisher/>
	<copyright/>
    <legalnotice/>
    <revhistory/>
    <revision/>
    <releaseinfo/>
  </t:titlepage-content>

  <t:titlepage-content side="verso">
  </t:titlepage-content>

  <t:titlepage-separator>
  </t:titlepage-separator>

  <t:titlepage-before side="recto">
  </t:titlepage-before>

  <t:titlepage-before side="verso">
    <img align ="left" src="stylesheet/gnucash-icon.png" alt="GnuCash Logo"/>
  </t:titlepage-before>
</t:titlepage>
</t:templates>
